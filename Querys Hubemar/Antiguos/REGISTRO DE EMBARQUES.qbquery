SELECT P.CODIGO_PRODUCTO, ED.NUMERO_PEDIDO, PA.NOMBRE_PAIS, DF.CANTIDAD, DF.VALOR_UNITARIO, DF.TOTAL_ITEM



FROM DETALLES_FACTURAS DF
LEFT OUTER JOIN PRODUCTOS  P   ON P.ID_PRODUCTO=DF.ID_PRODUCTO
LEFT OUTER JOIN PAISES PA ON PA.CODIGO_PAIS=DF.CODIGO_PAIS_ORIGEN
LEFT OUTER JOIN CABEZA_FACTURAS CF ON CF.ID_CABEZA_FACTURA=DF.ID_CABEZA_FACTURA
LEFT OUTER JOIN ESTADOS_DO ED ON ED.NUMERO_DO=CF.NUMERO_DO

WHERE ED.ID_IMPORTADOR=1010
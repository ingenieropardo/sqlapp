SELECT ( FV.TOTAL_GASTOS_DO + FV.VALOR_GRAVAMEN_FINANCIERO ) PAGO_TERCEROS, FV.SUBTOTAL_FACTURA INGRESOS_PROPIOS, NULL, ED.NUMERO_PEDIDO, ED.NUMERO_DO, ED.NUMERO_ORDEN_COMPRA
FROM  ESTADOS_DO ED
LEFT OUTER JOIN EXPORTACIONES        EX ON EX.NUMERO_DO=ED.NUMERO_DO
LEFT OUTER JOIN ( SELECT FVD.NUMERO_DO, F1.NUMERO_FACTURA_VENTA, F1.FECHA_FACTURA_VENTA, F1.TOTAL_GASTOS_DO ,F1.VALOR_GRAVAMEN_FINANCIERO, F1.SUBTOTAL_FACTURA   FROM FACTURAS_DOS FVD
                  INNER JOIN FACTURAS_VENTAS F1 ON FVD.ID_FACTURA_VENTA = F1.ID_FACTURA_VENTA WHERE F1.VERIFICADO='S' AND F1.ANULADO='N' ) FV ON FV.NUMERO_DO=ED.NUMERO_DO

WHERE ED.ID_IMPORTADOR="244" AND ED.TIPO_REGIMEN="E" AND ED.ANULADO="N"
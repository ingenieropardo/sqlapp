SELECT IM.NOMBRE_IMPORTADOR, IM.DIRECCION, M.NOMBRE_MUNICIPIO, CF.CODIGO_CONDICION_ENTREGA, COUNT(ED.NUMERO_DO) OPERACIONES

FROM ESTADOS_DO ED
INNER JOIN CABEZA_FACTURAS CF ON ED.NUMERO_DO=CF.NUMERO_DO
INNER JOIN IMPORTADORES IM ON IM.ID_IMPORTADOR=ED.ID_IMPORTADOR
INNER JOIN MUNICIPIOS M    ON M.CODIGO_MUNICIPIO = IM.CIUDAD_DOMICILIO
WHERE ED.FECHA_APERTURA>="2019-02-01" AND ED.ANULADO="N" AND ED.TIPO_REGIMEN='I'
GROUP BY 1,4
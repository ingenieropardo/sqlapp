select DATE_FORMAT(FD.FECHA_ACEPTACION, '%d/%m/%Y') FECHA, ED.NUMERO_DO, FD.ACEPTACIONES
from estados_do ED
INNER JOIN ( SELECT NUMERO_DO, FECHA_ACEPTACION, COUNT(NUMERO_ACEPTACION) ACEPTACIONES FROM FORMULARIOS_DIS GROUP BY 1 ) FD ON FD.NUMERO_DO=ED.NUMERO_DO
where fecha_aCEPTACION>=
"2017-11-01" AND ANULADO="N" AND TIPO_REGIMEN="I"
GROUP BY 1
ORDER BY 1
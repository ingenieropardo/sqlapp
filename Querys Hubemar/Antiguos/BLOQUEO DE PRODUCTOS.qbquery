SELECT I.NUMERO_IDENTIFICACION, P.ID_PRODUCTO, 'CREADO', NULL, CODIGO_PRODUCTO, REFERENCIA, CODIGO_POSICION, NULL,
NOMBRE_COMERCIAL_PROD, MARCA_COMERCIAL_PROD, DESCRIPCION_MINIMA, DESCRIPCION, CLASE, TIPO, MODELO, OTRAS_CARACTERISTICAS,
P.ID_EXPORTADOR, P.APROBADO, P.ESTADO, OBSERVACIONES, P.USUARIO_CREACION, P.FECHA_CREACION, P.USUARIO_MODIFICACION, P.FECHA_MODIFICACION

FROM PRODUCTOS P
LEFT OUTER JOIN IMPORTADORES I ON I.ID_IMPORTADOR=P.ID_IMPORTADOR
WHERE P.ID_IMPORTADOR=46
ORDER BY P.CODIGO_PRODUCTO, P.FECHA_MODIFICACION

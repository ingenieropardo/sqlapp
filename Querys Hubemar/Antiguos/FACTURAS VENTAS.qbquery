select E.NUMERO_IDENTIFICACION, E.NOMBRE_ENTE, ED.TIPO_REGIMEN, FV.CODIGO_SUCURSAL, FV.NUMERO_FACTURA_VENTA,
FV.FECHA_FACTURA_VENTA, FV.FECHA_VENCIMIENTO, FV.TOTAL_ANTICIPOS, FV.TOTAL_GASTOS_DO, FV.VALOR_GRAVAMEN_FINANCIERO

FV.* from facturas_ventas FV
LEFT OUTER JOIN ENTES E ON E.ID_ENTE=FV.ID_ENTE
LEFT OUTER JOIN FACTURAS_DO FD ON FD.ID_FACTURA_VENTA=FV.ID_FACTURA_VENTA
LEFT OUTER JOIN ESTADOS_DO ED  ON FD.NUMERO_DO=ED.NUMERO_DO
WHERE FV.NUMERO_FACTURA_VENTA="5860"
SELECT  @curRow := @curRow + 1 AS '#', ED.NUMERO_DO 'NUMERO DO', FD.CANTIDAD 'CANTIDAD', I.NOMBRE_IMPORTADOR 'NOMBRE IMPORTADOR',
  FD.NUMERO_ACEPTACION 'NUMERO ACEPTACION', DATE_FORMAT(FD.FECHA_LEVANTE,'%d/%m/%Y') 'FECHA LEVANTE'

  FROM    ESTADOS_DO ED

  INNER JOIN IMPORTADORES I ON I.ID_IMPORTADOR=ED.ID_IMPORTADOR
  INNER JOIN IMPORTACIONES IM ON ED.NUMERO_DO=IM.NUMERO_DO
  INNER JOIN ( SELECT NUMERO_DO, COUNT(NUMERO_ACEPTACION) CANTIDAD, NUMERO_ACEPTACION, FECHA_LEVANTE FROM FORMULARIOS_DIS GROUP BY 1)
    FD ON FD.NUMERO_DO=ED.NUMERO_DO
  INNER JOIN (SELECT @curRow := 0) r